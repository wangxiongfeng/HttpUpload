<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical"
    tools:context="projectnine.cn.com.httpupload.MainActivity">

    <Button
        android:id="@+id/getbnt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="get" />

    <Button
        android:id="@+id/postbnt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="post" />

    <Button
        android:id="@+id/alibaba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="V-LAYOUT" />

    <projectnine.cn.com.httpupload.CustomView
        android:id="@+id/customview"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="#ff7" />


</LinearLayout>
